<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <title>Success Page</title>
    </head>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">We received your order!</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block cacheable="false" class="Purpletree\Marketplace\Block\Checkout\Success" name="purpletree_checkout_success" template="checkout/success.phtml"/>
        </referenceContainer>
    </body>
</page>