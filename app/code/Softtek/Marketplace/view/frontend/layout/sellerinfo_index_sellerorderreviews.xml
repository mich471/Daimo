<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Softtek\Marketplace\Block\Order\SellerOrderReview" name="sales.order.history" cacheable="false">
                <container name="sales.order.history.info" as="info" label="Seller Order History Info"/>
                <container name="sales.order.history.extra.column.header" as="extra.column.header" label="Order History Extra Column Header"/>
            </block>
        </referenceContainer>
    </body>
</page>
